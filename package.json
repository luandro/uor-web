{
  "name": "guiala-web",
  "description": "Isomorphic starterkit with server-side React rendering.",
  "version": "0.0.1",
  "license": "BSD-3",
  "repository": {
    "type": "git",
    "url": "https://github.com/Luandro/guiala-web.git"
  },
  "homepage": "https://github.com/Luandro/guiala-web",
  "keywords": [
    "react",
    "isomorphic",
    "starter",
    "boilerplate",
    "template",
    "webpack",
    "hapi"
  ],
  "main": "babel.server.js",
  "scripts": {
    "start": "NODE_PATH=\"./src\" node ./babel-server",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client.js",
    "production": "NODE_ENV=production npm run build && NODE_ENV=production npm run start",
    "pouchdb-server": "pouchdb-server",
    "redis": "redis-server /usr/local/etc/redis.conf",
    "watch-client": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client-watch.js && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.client-watch.js",
    "dev": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run redis\" \"npm run pouchdb-server\" \"npm run watch-client\" \"npm run start\"",
    "postinstall": "webpack -p --config ./webpack.client.js"
  },
  "dependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.33",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "falcor": "^0.1.15",
    "falcor-express": "^0.1.2",
    "falcor-router": "^0.2.12",
    "h2o2": "^5.0.0",
    "hapi": "^11.1.2",
    "history": "1.13.x",
    "immutable": "^3.7.6",
    "inert": "^3.2.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.0.0",
    "passport-local": "^1.0.0",
    "piping": "0.3.0",
    "pouchdb-find": "^0.8.0",
    "r-dom": "^2.1.0",
    "radium": "^0.15.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-helmet": "^2.2.0",
    "react-map-gl": "^0.6.1",
    "react-motion": "^0.3.1",
    "react-portal": "^1.4.1",
    "react-redux": "4.0.1",
    "react-router": "^1.0.0",
    "react-tap-event-plugin": "0.2.1",
    "redux": "^3.0.5",
    "redux-auth": "0.0.2",
    "redux-falcor": "^2.3.3",
    "redux-search": "^0.2.0",
    "redux-simple-router": "1.0.0",
    "redux-thunk": "^1.0.2",
    "ua-parser-js": "^0.7.10",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.29",
    "concurrently": "^1.0.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.0",
    "eslint-plugin-react": "^3.11.3",
    "json-loader": "^0.5.4",
    "react-transform": "0.0.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "webpack-dev-server": "^1.14.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
